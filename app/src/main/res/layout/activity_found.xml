<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.meetafriend.meetafriend.Found"
    android:background="#ffffff"
    android:layout_marginBottom="5dp">

    <include
        android:id="@+id/tool_bar"
        layout="@layout/toolbar"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Age"
            android:id="@+id/age"
            android:textSize="15dp"
            android:layout_marginLeft="51dp"
            android:layout_marginStart="51dp"
            android:layout_marginTop="159dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Gender"
            android:id="@+id/gender"
            android:textSize="15dp"
            android:layout_below="@+id/age"
            android:layout_alignLeft="@+id/age"
            android:layout_alignStart="@+id/age"
            android:layout_marginTop="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Interests"
            android:id="@+id/interests"
            android:layout_marginTop="99dp"
            android:layout_below="@+id/genderMatch"
            android:textSize="20dp"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ageMatch"
            android:layout_alignTop="@id/age"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/genderMatch"
            android:layout_alignTop="@+id/gender"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/interestsMatch"
            android:layout_marginTop="39dp"
            android:layout_below="@+id/interests"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/usernameMatch"
            android:layout_above="@+id/ageMatch"
            android:layout_marginBottom="34dp"
            android:layout_alignStart="@+id/interestsMatch" />

    </RelativeLayout>


</RelativeLayout>
